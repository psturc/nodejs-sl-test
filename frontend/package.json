{
  "name": "todo-frontend",
  "version": "1.0.0",
  "dependencies": {
    "axios": "^1.6.2",
    "env-cmd": "^10.1.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-scripts": "5.0.1"
  },
  "scripts": {
    "start": "env-cmd -f .dev.env react-scripts start",
    "build": "rm -rf build && env-cmd -f .prod.env react-scripts build",
    "build:dev": "rm -rf build && env-cmd -x -f .dev.env react-scripts build"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
